연습문제 5
import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int count = 0;

        for(int i = 0 ; i < 10 ; i++){
            int n = scanner.nextInt();
            if (n % 2 == 0) count++;
        }
        System.out.printf("입력받은 짝수는 %d개입니다.",count);


    }
}

자가진단 5
import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int Muti3=0;
        int Muti5=0;
        for (int i = 0; i < 10; i++) {
            int num = scanner.nextInt();
            if(num%3==0 && num%5==0){
                Muti3++;
                Muti5++;
            }else if(num%5==0){
                Muti5++;
            }else if (num%3==0){
                Muti3++;
            }
        }
        System.out.printf("Multiples of 3 : %d\nMultiples of 5 : %d\n", Muti3, Muti5);


    }
}

연습문제 6
import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int sum = 0;
        double avg = 0;
        int count = 0;

        for (int i =0; i<5; i++){
            int n = scanner.nextInt();
            sum += n;
            count++;
        }
        avg = (double)sum / count;
        System.out.println("총점 : "+sum);
        System.out.printf("평균 : %.1f",avg);
    }
}

자가진단 6
import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int n = scanner.nextInt();
        int sum = 0;
        double avg = 0.0;

        for(int i = 0; i < n; i++){
            int num = scanner.nextInt();
            sum += num;
            avg = (double)sum / (double)n;
        }
        System.out.printf("avg : %.1f\n",avg);
        if(avg >= 80.0) {
            System.out.println("pass");}
        else {
            System.out.println("fail");
        };

    }
}

연습문제 7
import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        for(int i = 1 ; i <= 5 ; i++){
            for(int j = 0 ; j < 5 ; j++){
                System.out.print(i+" ");
            }
            System.out.println();
        }
        System.out.println();
        for(int i = 1 ; i <= 5 ; i++){
            for(int j = 1 ; j <= 5 ; j++){
                System.out.print(j+" ");
            }
            System.out.println();
        }
    }
}

