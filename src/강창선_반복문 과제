연습문제 7
import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        char c = 97;
        int num = 1;
        for (int i = 1; i <= 4; i++) {
            for (int j = 0; j < i; j++) {
                System.out.print(c+" ");
                c++;
            }

            for (int j = 4; j >= i; j--) {
                System.out.print(num+" ");
                num++;
            }
            System.out.println();
        }
    }
}

자가진단 7
import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int n = scanner.nextInt();
        int num = 1;
        char c = 65;

        for (int i = 0; i < n; i++) {
            for (int j = n; i < j; j--) {
                System.out.print(num + " ");
                num++;
            }
            for (int j = 1; j <= i+1; j++) {
                System.out.print(c + " ");
                c++;
            }
            System.out.println();
        }
    }
}

형성평가 1
import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int sum = 0;
        double avg = 0;
        int n = 1;
        int count = 0;

        for (int i = 0; i < 20; i++) {
            n = scanner.nextInt();
            if(n==0){
                break;
            }
            sum += n;
            count++;
            avg = sum / count;
        }
        System.out.printf("%d %d", sum, (int) avg);


    }
}

형성평가 2
import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int n = scanner.nextInt();
        int count = 1;

        for (int i = 1; i <= 9; i++) {
            if (n*i % 10 != 0 && (n*i) < 100) {
                System.out.print(n * i + " ");

            } else if (n*i % 10 == 0) {
                System.out.println(n * i + " ");
                break;
            } else if(n*i > 100){
                break;
            }
        }

    }
}

