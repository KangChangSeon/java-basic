형성평가 6
import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        String c = "  ";
        int n = scanner.nextInt();

        for (int i = n - 1; i >= 0; i--) {
            int num = 1;
            for (int j = 0; j < i; j++) {
                System.out.print(c);
            }
            for (int j = n; j > i; j--) {
                System.out.print(num + " ");
                num++;
            }
            System.out.println();
        }
    }
}

형성평가 7
import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int n = scanner.nextInt();
        char c = 65;
        int count = 0;

        for(int i = 0; i < n; i++){
            for(int j = n; j>i; j--){
                System.out.print(c+" ");
                c++;
            }
            for(int j = 1; j<=i; j++){
                System.out.print(count+" ");
                count++;
            }
            System.out.println();
        }
    }
}

