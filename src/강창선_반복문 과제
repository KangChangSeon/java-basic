형성평가 2
import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        int num = 0;
        int odd = 0;
        int even = 0;

        while(true){
            num = scan.nextInt();
            if(num <= 0){
                break;
            }
            if(num % 2 == 0){
                even++;
            }else{
                odd++;
            }
        }
        System.out.printf("odd : %d\neven : %d", odd, even);
    }
}

형성평가 3
import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        int num = 0;
        int sum = 0;
        double avg = 0;
        int i = 1;

        while(true){
            num = scan.nextInt();
            if(num < 0 || num > 100){
                break;
            }
            else {
                sum += num;
                avg = (double) sum / i++;
            }
        }
        System.out.printf("sum : %d\navg : %.1f", sum, avg);

    }
}

형성평가 4
import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        int num = 0;
        int i = 0;

        while(true){
            num = scan.nextInt();
            if(num == 0){
                break;
            }
            if(num%3!=0 && num%5!=0){
                i++;
            }

        }
        System.out.println(i);

    }
}

형성평가 5
import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        int Base = 0;
        int Height = 0;
        String go = "";

        while (true) {
            Base = scan.nextInt();
            Height = scan.nextInt();
            double TW = (double)Base * Height / 2;
            System.out.printf("Base = Height = Triangle width = %.1f\n", TW);
            System.out.print("Continue? ");
            go = scan.next();
            if (!go.equalsIgnoreCase("Y") || !go.equalsIgnoreCase("y")) {
                break;
            }

        }

    }
}


