import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int n = scanner.nextInt();
        String c = "*";
        for (int i = 1; i <= n; i++) {
            for (int j = 1; j <= i; j++) {
                System.out.print(c);
            }
            System.out.println();
        }
        for(int i = n-1; i > 0; i--){
            for (int j = i; j > 0; j--) {
                System.out.print(c);
            }
            System.out.println();
        }

    }
}

