연습문제 2
import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int num = 1;
        int sum = 0;
        while(true){
            sum += num;
            num++;
            if(num>10){
                break;
            }
        }
        System.out.println("1부터 10까지의 합 = "+sum);
        System.out.println("while문이 끝난 후의 num의 값 = "+num);
    }
}

자가진단 2
import javax.management.openmbean.CompositeType;
import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        int num = scan.nextInt();
        int sum = 0;
        while(num != 0){
            sum+=num;
            num--;
        }
        System.out.println(sum);

    }
}

연습문제 3
import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int score = 0;
        while(true){
            System.out.print("점수를 입력하세요. ");
            score = scanner.nextInt();
            if(score < 0 || score > 100){
                break;
            }else if(score>=80){
                System.out.println("축하합니다. 합격입니다.");
            } else if(score<80){
                System.out.println("죄송합니다. 불합격입니다.");
            }


        }
    }
}

자가진단 3
import javax.management.openmbean.CompositeType;
import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        int a=1;
        while(a != 0){
            System.out.print("number? ");
            a = scan.nextInt();
            if(a > 0){
                System.out.println("positive integer");
            } else if(a < 0){
                System.out.println("negative number");
            }

        }

    }
}

연습문제 4
import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int sum = 0;
        double avg = 0;
        int count = 0;
        while(true){
            int num = scanner.nextInt();
            sum += num;

            if(num==0){
                avg = (double)sum/ count;
                System.out.println("입력된 자료의 개수 = " + count);
                System.out.println("입력된 자료의 합계 = " + sum);
                System.out.printf("입력된 자료의 평균 = %.2f",avg);
                break;
            }
            count++;
        }

    }
}

자가진단 4
import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        int a = 1;
        int sum = 0;
        double avg = 0.0;
        int tmp = 1;

        while (a < 100) {
            a = scan.nextInt();
            sum += a;
            avg = (double) sum / tmp;
            tmp++;
        }

        System.out.printf("%d\n%.1f", sum, avg);
    }
}

연습문제 5
import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int sum = 0;
        double avg = 0;
        int count = 0;
        while (true) {
            int num = scanner.nextInt();
            if (num == 0) {
                break;
            }else if(num%2!=0){
                count++;
                sum += num;
                avg = sum / count;
            }
        }
        System.out.println("홀수의 합 = "+sum);
        System.out.printf("홀수의 평균 = %d",(int)avg);
    }
}

자가진단 5
import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        int a=0;
        while(a != -1){
            a = scan.nextInt();
            if (a%3==0){
                System.out.println(a/3);
            }
        }
    }
}

연습문제 6
import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int num = 0;
        while (num!=4) {
            System.out.println("1. 입력하기\n" +
                    "2. 출력하기\n" +
                    "3. 삭제하기\n" +
                    "4. 끝내기\n" +
                    "작업할 번호를 선택하세요. ");
            num = scanner.nextInt();
            switch (num) {
                case 1:
                    System.out.println("입력하기를 선택하였습니다.");
                    continue;
                case 2:
                    System.out.println("출력하기를 선택하였습니다.");
                    continue;
                case 3:
                    System.out.println("삭제하기를 선택하였습니다.");
                    continue;
                case 4:
                    System.out.println("끝내기를 선택하였습니다.");
                    break;
                default:
                    System.out.println("잘못 입력하였습니다.");
            }
        }
    }
}

자가진단 6
import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        int num = 1;
        while (num <= 4 && num >= 1) {
            System.out.println("1. Korea\n" +
                    "2. USA\n" +
                    "3. Japan\n" +
                    "4. China");
            System.out.print("number? ");
            num = scan.nextInt();
            switch (num) {
                case 1:
                    System.out.println("\nSeoul\n");
                    break;
                case 2:
                    System.out.println("\nWashington\n");
                    break;
                case 3:
                    System.out.println("\nTokyo\n");
                    break;
                case 4:
                    System.out.println("\nBeijing\n");
                    break;
                default:
                    System.out.println("\nnone\n");
            }

        }
    }
}

