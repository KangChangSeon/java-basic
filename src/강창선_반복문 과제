형성평가 9
import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int n = scanner.nextInt();
        String spc = "  ";
        char c = '#';

        for(int i = 0; i < 2 * n - 1; i++){
            if(i < n) {
                for(int j = 0; j < i + 1; j++){
                    System.out.print(c + " ");
                }
            }else {
                int bottom = i - n + 1;

                for(int j = 0; j < bottom; j++){
                    System.out.print(spc);
                }

                for(int j = 0; j < n - bottom; j++){
                    System.out.print(c + " ");
                }
            }
            System.out.println();
        }
    }
}

형성평가 10
import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int n = scanner.nextInt();
        int num = 1;
        for (int i = 0; i < n; i++) {
            for (int j = 0; j < n; j++) {
                if (num % 2 == 0) {
                    num++;
                }
                if (num > 10) {
                    num = 1;
                }
                System.out.print(num + " ");
                num++;

            }
            System.out.println();
        }
    }
}

