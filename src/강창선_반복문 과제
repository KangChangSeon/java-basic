import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        String c = " ";
        String star = "*";
        int n = scanner.nextInt();

        for(int i = 0; i < n; i++){
            for (int j = 0; j < i; j++){
                System.out.print(" ");
            }
            for (int j = 0; j < (n*2)-1-(i*2); j++){
                System.out.print("*");
            }
            System.out.println();
        }

        for(int i = n-2; i >= 0; i--){
            for (int j = 0; j < i; j++){
                System.out.print(" ");
            }
            for (int j = 0; j < (n*2)-1-(i*2); j++){
                System.out.print("*");
            }
            System.out.println();
        }


    }
}

