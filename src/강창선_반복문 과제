    }
}

형성평가 10
import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int a = scanner.nextInt();
        int b = scanner.nextInt();


        if(a<b){
            for (int i = 1; i <= 9; i++) {
                for (int j = a; j <= b; j++) {
                    System.out.printf("%d * %d = %2d   ", j, i, j * i);
                }
                System.out.println();
            }
        }
        else{
            // a = 5, b = 3
            for (int i = 1; i <= 9; i++) {
                for (int j = a; j >= b; j--) {
                    System.out.printf("%d * %d = %2d   ", j, i, j * i);
                }
                System.out.println();
            }
        }
    }
}

- 반복제어문 3 -

연습문제 1
import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int n = scanner.nextInt();
        int sum = 0;
        int count = 0;

        for (int i = 1; i <= n; i++) {
            sum += i;
            count++;
            if (sum > n) {
                System.out.printf("%d %d", count, sum);
                break;
            }
        }

    }
}

자가진단 1
import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int n = scanner.nextInt();
        int sum = 0;
        int count = 0;
        for (int i = 1; i <= n; i++){
            if(i%2!=0){
                sum+=i;
                count++;
                if(sum>=n){
                    System.out.printf("%d %d",count,sum);
                    break;
                }
            }
        }
    }
}

